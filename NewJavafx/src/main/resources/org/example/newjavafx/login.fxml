<?xml version="1.0" encoding="UTF-8"?>

<?import java.lang.*?>
<?import java.util.*?>
<?import javafx.scene.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>

<?xml version="1.0" encoding="UTF-8"?>
<AnchorPane xmlns="http://javafx.com/javafx"
            xmlns:fx="http://javafx.com/fxml"
            fx:controller="org.example.newjavafx.LoginController">
    <children>
        <TextField fx:id="usernameField" layoutX="14.0" layoutY="30.0" />
        <PasswordField fx:id="passwordField" layoutX="14.0" layoutY="60.0" />
        <Button text="Login" onAction="#login" layoutX="14.0" layoutY="90.0" />
        <Text fx:id="loginError" layoutX="14.0" layoutY="120.0" />
    </children>
</AnchorPane>